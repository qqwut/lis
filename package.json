{
  "name": "phxpartners-frontend",
  "version": "1.2.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "app": "npm run build-prod && NODE_ENV=production PORT=8080 node index.js",
    "app-ais-prod": "npm run build-prod && NODE_ENV=production_ais node app.js",
    "web": "ng serve --environment=iot --ssl true --ssl-key ./key1.pem --ssl-cert ./cert1.pem --proxy-config ./proxy_map/proxy-local.conf.json",
    "web-iot": "ng serve --environment=iot --ssl true --ssl-key ./key1.pem --ssl-cert ./cert1.pem --proxy-config ./proxy_map/proxy-iot.conf.json",
    "web-vpn": "ng serve --host 202.129.207.230 --ssl true --ssl-key ./key1.pem --ssl-cert ./cert1.pem --proxy-config ./proxy_map/proxy-vpn.conf.json",
    "web-vpn-public": "ng serve --host 202.129.207.230 --ssl true --ssl-key ./key1.pem --ssl-cert ./cert1.pem --proxy-config ./proxy_map/proxy-vpn-public-mock-login.conf.json",
    "web-dev": "gulp less && ng serve --ssl true --ssl-key ./key1.pem --ssl-cert ./cert1.pem --proxy-config ./proxy_map/proxy-local.conf.json LIMIT=4096 increase-memory-limit",
    "web-devv": "gulp less && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve --ssl true --ssl-key ./key1.pem --ssl-cert ./cert1.pem --proxy-config ./proxy_map/proxy-local.conf.json",
    "web-node-iot": "gulp less && ng serve --ssl true --ssl-key ./key1.pem --ssl-cert ./cert1.pem --proxy-config ./proxy_map/proxy-node-iot.conf.json",
    "web-hama": "ng serve --host 0.0.0.0 --ssl true --ssl-key ./key1.pem --ssl-cert ./cert1.pem --proxy-config ./proxy_map/proxy-hama.conf.json",
    "web-hama-his": "ng serve --host 0.0.0.0 --ssl true --ssl-key ./key1.pem --ssl-cert ./cert1.pem --proxy-config ./proxy_map/proxy-his-hama.conf.json",
    "web-iot-ph2": "ng serve --host 0.0.0.0 --ssl true --ssl-key ./key1.pem --ssl-cert ./cert1.pem --proxy-config ./proxy_map/proxy-iot-ph2.conf.json",
    "start": "node server.js",
    "watch-ts": "ng build --watch --dev --aot false",
    "lint": "tslint \"src/**/*.ts\"",
    "build-dev": "gulp less && ng build --dev --aot=false",
    "build-vpn": "gulp less && ng build --test",
    "build-prod": "gulp less && ng build --prod  --aot=false",
    "build-patch": "npm version patch && node ./replace.build.js && npm run build-prod && tar -cvf public.$(git describe).tar.gz public",
    "pack-prod": "tar --exclude='./.git*' --exclude='./.log/*' --exclude='./src' -cvf phxpartners-frontend.tar.gz ../phxpartners-frontend"
  },
  "author": "entronica team.",
  "private": true,
